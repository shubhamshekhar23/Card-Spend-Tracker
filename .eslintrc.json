{
  "extends": "next/core-web-vitals",
  "rules": {
    // for a file to have maximum lines
    "max-len": [
      "error",
      {
        "code": 200,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true,
        "ignoreComments": true,
        "ignoreTrailingComments": true
      }
    ]
  }
}
